<% if(success && success.length) { %>
<div class="alert alert-<% if(success == "You have successfuly deleted Listing" || success == "You have successfuly delete Review" || success == "You are now logged out") {%>danger<%} else {%>success<%}%> alert-dismissible fade show my-2" role="alert">
    <strong><%= success %></strong>
    <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
</div>
<% } %>


<!-- # for comment the scriptlet tags -->

<% if(error && error.length) { %>
<div class="alert alert-danger alert-dismissible fade show my-2" role="alert">
    <strong><%= error %></strong>
    <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
</div>
<% } %>